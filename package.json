    "antd": "^4.18.8",
    "antd-form-builder": "^2.1.4",
    "axios": "^0.26.1",
    "moment": "^2.29.1",
    "react": "17.x",
    "react-dom": "17.x",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "redux": "^4.1.2",
    "umi": "^3.5.20"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
